<h1>{{username}}'s EZ List Page</h1>

<div class="row row-cols-1 row-cols-md-3 g-2 mt-3">
  {{#each ezlists}}
    <div class="col">
      <div class="card">
        <div class="card-body">
          <a href="/api/ezlists/{{id}}"><h3>{{name}}</h3></a>
        </div>
      </div>
    </div>
  {{/each}}
</div>